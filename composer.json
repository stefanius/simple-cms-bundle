{
    "name": "stef/simple-cms-bundle",
    "license": "MIT",
    "type": "project",
    "description": "Easy to use backoffice / CMS bundle",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.10.2",
                "dist": {
                    "url": "https://code.jquery.com/jquery-1.10.2.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/stefanius/generate-fixtures-bundle"
        }
    ],
    "require": {
        "php": ">=5.5",
        "phpunit/phpunit": "~4.1",
        "twig/extensions": "~1.3",
        "incenteev/composer-parameter-handler": "~2.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "twbs/bootstrap": "~3.3",
        "jquery/jquery":  "1.10.*",
        "braincrafted/bootstrap-bundle": "~2.1",
        "nelmio/alice": "~2.0",
        "egeloen/ckeditor-bundle": "~2.0",
        "stefanius/manipulation": "~0",
        "stef/slug-manipulation": "dev-master",
        "stef/key-value-manipulation": "dev-master",
        "jms/serializer-bundle": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Stef\\SimpleCmsBundle\\": "src/Stef/SimpleCmsBundle/" }
    }
}
