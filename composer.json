{
    "name": "stefanius/simple-cms-bundle",
    "license": "MIT",
    "type": "library",
    "description": "Easy to use backoffice / CMS bundle",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.10.2",
                "dist": {
                    "url": "https://code.jquery.com/jquery-1.10.2.js",
                    "type": "file"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "~3.0",
        "doctrine/orm": "~2.5",
        "doctrine/dbal": "~2.5",
        "doctrine/doctrine-bundle": "~1.2",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "twig/extensions": "~1.3",
        "incenteev/composer-parameter-handler": "~2.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "twbs/bootstrap": "~3.3",
        "symfony/form": "~3.0",
        "jquery/jquery":  "1.10.*",
        "braincrafted/bootstrap-bundle": "~2.1",
        "nelmio/alice": "~2.0",
        "egeloen/ckeditor-bundle": "~4.0",
        "stefanius/manipulation": "~0",
        "stefanius/slugifier": "~0",
        "jms/serializer-bundle": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "scripts": {
        "post-install-cmd": [

        ],
        "post-update-cmd": [

        ]
    },
    "autoload": {
        "psr-4": { "Stefanius\\SimpleCmsBundle\\": "src/Stefanius/SimpleCmsBundle/" }
    }
}
